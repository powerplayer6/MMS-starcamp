git init	- empty repo
git add	.	- stages files
git status	- shows staged/un files

git config --global user.name user1
git confirg --global user.email mail@example.com

git commit -m	- creates commit

git log		- commit history

git checkout -- file1	- load file1 from latest commit

git checkout COMMIT_HASH	- load all files from commit with given hash

git checkout --		- loads all files from latest commit

git checkout master	- loads master commit
